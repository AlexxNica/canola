@namespace(glyph#nyadeva_viramadeva_jadeva) {

point > center {
    on: scale_B_on;
    in: scale_B_in;
    out: scale_B_out;

}

penstroke#stem point > center {
    on: scale_stem_on;
    in: scale_stem_in;
    out: scale_stem_out;

}

@import 'serif.cps';
}




